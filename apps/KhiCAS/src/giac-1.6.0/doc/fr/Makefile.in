install-dist_thelangdocDATA: $(dist_thelangdoc_DATA)
	@$(NORMAL_INSTALL)
	@list='$(dist_thelangdoc_DATA)'; test -n "$(thelangdocdir)" || list=; \
	if test -n "$$list"; then \
	  echo " $(MKDIR_P) '$(DESTDIR)$(thelangdocdir)'"; \
	  $(MKDIR_P) "$(DESTDIR)$(thelangdocdir)" || exit 1; \
	fi; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(thelangdocdir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(thelangdocdir)" || exit $$?; \
	done

uninstall-dist_thelangdocDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(dist_thelangdoc_DATA)'; test -n "$(thelangdocdir)" || list=; \
	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	dir='$(DESTDIR)$(thelangdocdir)'; $(am__uninstall_files_from_dir)
