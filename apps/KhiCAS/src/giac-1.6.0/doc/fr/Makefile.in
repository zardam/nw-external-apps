all-data-local: $(HTML_STAMPS) $(PDF_LOCAL) 

install-data-local: 
	$(install_sh) -d $(DESTDIR)$(prefix)/share/giac/doc/$(the_LANG)
	$(INSTALL_DATA) $(PDF_LOCAL) $(DESTDIR)$(prefix)/share/giac/doc/$(the_LANG)
#	$(INSTALL_DATA) $(PS_LOCAL) $(DESTDIR)$(prefix)/share/giac/doc/$(the_LANG)
#	$(INSTALL_DATA) $(DVI_LOCAL) $(DESTDIR)$(prefix)/share/giac/doc/$(the_LANG)
	$(INSTALL_DATA) $(HTML_LOCAL) $(DESTDIR)$(prefix)/share/giac/doc/$(the_LANG)
	for dd in $(HTML_DIRS_LOCAL); do \
		$(install_sh) -d $(DESTDIR)$(prefix)/share/giac/doc/$(the_LANG)/$$dd ; \
	done
	for dd in $(HTML_DIRS_LOCAL); do \
		$(INSTALL_DATA) $(srcdir)/$$dd/* $(DESTDIR)$(prefix)/share/giac/doc/$(the_LANG)/$$dd ; \
	done
#	export LD_LIBRARY_PATH=$(DESTDIR)/lib ; $(DESTDIR)/bin/icas --rebuild-help-cache
	$(INSTALL_DATA) $(dist_thelangdoc_DATA) $(DESTDIR)$(prefix)/share/giac/doc/$(the_LANG)

@DOC_PROGS_TRUE@.tex.pdf:
@DOC_PROGS_TRUE@	$(RUN_PDFLATEX) $<
@DOC_PROGS_TRUE@	$(RUN_PDFLATEX) $<
@DOC_PROGS_TRUE@	if $(EGREP) -q -e '\\makeindex' $<; then \
@DOC_PROGS_TRUE@		$(RUN_MAKEINDEX) `echo $@ | sed -e s/\.dvi/\.idx/` ; \
@DOC_PROGS_TRUE@	fi
@DOC_PROGS_TRUE@	$(RUN_PDFLATEX) $<

@DOC_PROGS_TRUE@.tex.dvi:
@DOC_PROGS_TRUE@	$(RUN_LATEX) $<
@DOC_PROGS_TRUE@	$(RUN_LATEX) $<
@DOC_PROGS_TRUE@	if $(EGREP) -q -e '\\makeindex' $<; then \
@DOC_PROGS_TRUE@		$(RUN_MAKEINDEX) `echo $@ | sed -e s/\.dvi/\.idx/` ; \
@DOC_PROGS_TRUE@	fi
@DOC_PROGS_TRUE@	$(RUN_LATEX) $<

@DOC_PROGS_TRUE@.dvi.ps:
@DOC_PROGS_TRUE@	$(DVIPS) -o $@ $<

@DOC_PROGS_TRUE@.tex.stamp:
#	$(RUN_LATEX2HTML) $< --address '$(GIAC_ADDRESS)'
@DOC_PROGS_TRUE@	$(RUN_HEVEA) $< 
@DOC_PROGS_TRUE@	tdir=`echo $< | sed -e 's/\.tex//'`; \
@DOC_PROGS_TRUE@	imagen -png -pdf -mag 2000 $$tdir; \
@DOC_PROGS_TRUE@	$(install_sh) -d $$tdir ; \
@DOC_PROGS_TRUE@	hacha $$tdir.html -o $$tdir/index.html ; \
@DOC_PROGS_TRUE@	touch $$tdir.png ; \
@DOC_PROGS_TRUE@	/bin/cp -f $$tdir*.png $$tdir
#	rm -f $$tdir/*.old $$tdir/*.pl $$tdir/images.??? $$tdir/WARNINGS
@DOC_PROGS_TRUE@	touch $@

@DOC_PROGS_TRUE@dist-hook:
@DOC_PROGS_TRUE@	for ddir in $(HTML_DIRS_LOCAL); do $(MAKE) $$ddir.stamp; done

@DOC_PROGS_FALSE@.tex.pdf:
@DOC_PROGS_FALSE@	if [ ! -f "$@" ]; then \
@DOC_PROGS_FALSE@		echo '*** Error: TeX is not installed, and $@ is missing' ; \
@DOC_PROGS_FALSE@		exit 1 ; \
@DOC_PROGS_FALSE@	fi

@DOC_PROGS_FALSE@.tex.dvi:
@DOC_PROGS_FALSE@	if [ ! -f "$@" ]; then \
@DOC_PROGS_FALSE@		echo '*** Error: TeX is not installed, and $@ is missing' ; \
@DOC_PROGS_FALSE@		exit 1 ; \
@DOC_PROGS_FALSE@	fi

@DOC_PROGS_FALSE@.tex.ps:
@DOC_PROGS_FALSE@	if [ ! -f "$@" ]; then \
@DOC_PROGS_FALSE@		echo '*** Error: TeX is not installed, and $@ is missing' ; \
@DOC_PROGS_FALSE@		exit 1 ; \
@DOC_PROGS_FALSE@	fi

@DOC_PROGS_FALSE@.tex.stamp:
@DOC_PROGS_FALSE@	docdir=`echo $< | sed -e 's/\.tex//'` ; \
@DOC_PROGS_FALSE@	if [ ! -f "$$docdir/index.html" ]; then \
@DOC_PROGS_FALSE@		echo '*** Error: latex2html is not installed, and $$docdir is missing' ; \
@DOC_PROGS_FALSE@		exit 1 ; \
@DOC_PROGS_FALSE@	fi

@DOC_PROGS_FALSE@dist-hook:
@DOC_PROGS_FALSE@	echo '*** Error: no tools to rebuild documentation is installed' ; \
@DOC_PROGS_FALSE@	exit 1

install-exec-hook: 
	mkdir -p $(DESTDIR)$(prefix)/share/giac/doc/fr/
	ln -sf ../giac.js $(DESTDIR)$(prefix)/share/giac/doc/fr/giac.js
	ln -sf ../giacwasm.js $(DESTDIR)$(prefix)/share/giac/doc/fr/
	ln -sf ../giacwasm.wasm $(DESTDIR)$(prefix)/share/giac/doc/fr/

# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:
